before hooked


public class hookedClass {

    public static String stringAppendChar(String s, char c) {
        if( s == null || s.isEmpty()) return s;
        StringBuilder sb = new StringBuilder(s).append(c);
        sb.append("-ello");
        return sb.toString();
    }
}


after hooked


public class hookedClass {

    public static String stringAppendChar(String s, char c) {
        if( s == null || s.isEmpty()) return s;
        StringBuilder sb = new StringBuilder(s).append(c);
        return sb.toString();
    }
}